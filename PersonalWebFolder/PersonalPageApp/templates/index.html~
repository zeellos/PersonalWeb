				<script type="text/javascript" src="static/js/circles.js"></script>
						         <script>
									var colors = [
											['#fff', '#2980b9'], ['#fff', '#2980b9'], ['#fff', '#2980b9'], ['#fff', '#2980b9']
										];
									for (var i = 1; i <= 6; i++) {
										var child = document.getElementById('circles-' + i),
											percentage = 50 + ( (5-i) * 10);
											
										Circles.create({
											id:         child.id,
											percentage: percentage,
											radius:     80,
											width:      15,
											number:   	percentage,
											text:       '%',
											colors:     colors[i - 1]
										});
									}
							
					</script>
